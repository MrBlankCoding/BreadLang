// Test array manipulation in LLVM backend
let arr: [Int] = [1, 2, 3]
print(arr.length)

// Append elements
arr.append(4)
arr.append(5)
print(arr.length)

// Access elements
for i in range(arr.length) {
    print(arr[i])
}

// Nested arrays
let matrix: [[Int]] = [[1, 2], [3, 4], [5, 6]]
print(matrix.length)
print(matrix[0].length)
print(matrix[1][1])

// Array of strings
let words: [String] = ["hello", "world"]
print(words.length)
print(words[0])
print(words[1])

words.append("!")
print(words.length)
print(words[2])