func add_ints(a: Int, b: Int) -> Int {
    return a + b
}

func local_computation() -> Int {
    var x: Int = 10
    var y: Int = 20
    var z: Int = x * y
    return z
}

func square(n: Int) -> Int {
    return n * n
}

func sum_range(n: Int) -> Int {
    var sum: Int = 0
    var i: Int = 0
    while i < n {
        sum = sum + i
        i = i + 1
    }
    return sum
}

func constants() -> Int {
    var a: Int = 5 + 3
    var b: Int = a * 2
    return b
}

print("Testing Stage 5 optimizations...")

var result1: Int = add_ints(10, 20)
print("add_ints(10, 20) = ")
print(result1)

var result2: Int = local_computation()
print("local_computation() = ")
print(result2)

var result3: Int = square(5)
print("square(5) = ")
print(result3)

var result4: Int = sum_range(10)
print("sum_range(10) = ")
print(result4)

var result5: Int = constants()
print("constants() = ")
print(result5)
