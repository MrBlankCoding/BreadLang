// Test error handling in LLVM backend
func divide(a: Int, b: Int) -> Int {
    if b == 0 {
        print("Error: Division by zero")
        return 0
    }
    return a / b
}

func array_access(arr: [Int], index: Int) -> Int {
    if index < 0 || index >= arr.length {
        print("Error: Array index out of bounds")
        return -1
    }
    return arr[index]
}

let result1: Int = divide(10, 2)
print(result1)

let result2: Int = divide(10, 0)
print(result2)

let numbers: [Int] = [1, 2, 3, 4, 5]
let valid: Int = array_access(numbers, 2)
print(valid)

let invalid: Int = array_access(numbers, 10)
print(invalid)