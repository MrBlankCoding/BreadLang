print("=== Phase 5 Advanced Types ===")

let nums: [Int] = [1, 2, 3]
print(nums.length)
print(nums[0])
nums.append(4)
print(nums.length)
print(nums[3])
print(nums[100])

let person: [String: String] = ["name": "John", "city": "NYC"]
print(person["name"])
print(person.name)
print(person["missing"])
print(person.length)

let age: Int? = nil
print(age)
age = 42
print(age)

let maybePerson: [String: String]? = nil
print(maybePerson?.name)
maybePerson = person
print(maybePerson?.name)
